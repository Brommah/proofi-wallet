{
  "name": "@proofi/api",
  "version": "0.0.1",
  "private": true,
  "description": "Auth API server",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit false --noEmitOnError false --skipLibCheck || true",
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/nodemailer": "^7.0.9",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "@cere-ddc-sdk/ddc-client": "^2.16.1",
    "@hono/node-server": "^1.19.9",
    "@polkadot/api": "^16.5.4",
    "@polkadot/keyring": "^14.0.1",
    "@polkadot/util": "^14.0.1",
    "@polkadot/util-crypto": "^14.0.1",
    "hono": "^4.11.7",
    "jose": "^6.1.3",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.13",
    "zod": "^3.23.0"
  }
}
