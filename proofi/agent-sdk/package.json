{
  "name": "@proofi/agent-sdk",
  "version": "1.0.0",
  "description": "SDK for agents to consume Proofi capability tokens and access user data",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "proofi",
    "agent",
    "sdk",
    "capability-token",
    "encryption",
    "ddc",
    "cere"
  ],
  "author": "Proofi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proofi/agent-sdk"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
